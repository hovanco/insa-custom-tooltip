ue}\n            >\n                <Table\n                    loading={loading}\n                    dataSource={dataSource}\n                    rowKey='_id'\n                    columns={[\n                        {\n                            title: '',\n                            dataIndex: '',\n                            key: 'check',\n                            render: ({ _id }: { _id: string }) => {\n                                const product = order.products.find(\n                                    (p: any) => p.productId._id === _id\n                                );\n                                const checked = !!product;\n                                return <Checkbox checked={checked} />;\n                            },\n                        },\n                        {\n                            title: 'Hnh nh',\n                            dataIndex: 'images',\n                            key: 'image',\n                            render: (images) => {\n                                if (images.length === 0)\n                                    return (\n                                        <Avatar\n                                            icon={<FileImageOutlined />}\n                                            size='large'\n                                            shape='square'\n                                        />\n                                    );\n                                return (\n                                    <Avatar\n                                        src={`${IMG_URL}${images[0]}`}\n                                        size='large'\n                                        shape='square'\n                                    />\n                                );\n                            },\n                        },\n                        {\n                            title: 'Tn',\n                            dataIndex: 'name',\n                            key: 'name',\n  